    "express-graphql": "^0.12.0",
    "express-jwt": "^6.1.0",
    "graphql": "^15.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.11",
    "npmlog": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@types/npmlog": "^4.1.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.13"
  }
